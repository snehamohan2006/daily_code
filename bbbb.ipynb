   "source": [
    "function reverse_number(n):\n",
    "    rev = 0\n",
    "    while n > 0:\n",
    "        last_digit = n % 10\n",
    "        rev = rev * 10 + last_digit\n",
    "        n = n // 10\n",
    "    return rev\n",
    "reverse_num"
   ]
