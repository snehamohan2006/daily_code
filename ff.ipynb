    "def fibonocci(n):\n",
    "    a=0\n",
    "    b=1\n",
    "    print(\"fibonocci series\")\n",
    "    for i in range(n):\n",
    "        print(a,end=\"\")\n",
    "        next_number=a+b\n",
    "        a=b\n",
    "        b=next_number\n",
    "\n",
    "num=int(input(\"enter how many numbers you want:\"))\n",
    "fibonocci(num)\n",
    "        \n"
   ]
