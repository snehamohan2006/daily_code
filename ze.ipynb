   "source": [
    "n=int(input(\"enter the number:\"))\n",
    "count=0\n",
    "while n>0:\n",
    "    digit=n%10\n",
    "    if digit==0:\n",
    "        count=count+1\n",
    "    n=n//10\n",
    "\n",
    "print(\"number of zeroes\",count)\n",
    "    \n",
    "\n"
