   "source": [
    "n=int(input(\"enter the numbers:\"))\n",
    "even_count=0\n",
    "odd_count=0\n",
    "while n>0:\n",
    "    digit=n%10\n",
    "    if  digit%2==0:\n",
    "        even_count=even_count+1\n",
    "    else:\n",
    "        odd_count=odd_count+1\n",
    "    n=n//10\n",
    "\n",
    "print(\"count even\",even_count)\n",
    "print(\"count odd\",odd_count)"
   ]
