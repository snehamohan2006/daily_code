   "source": [
    "n=int(input(\"enter the number:\"))\n",
    "original=n\n",
    "reversed_number=0\n",
    "while n>0:\n",
    "    digit=n%10\n",
    "    reversed_num=reversed_num*10+digit\n",
    "    n=n//10\n",
    "\n",
    "if reversed_num==original:\n",
    "    print(\"palindrome num\")\n"
   ]
