   "source": [
    "#arm strong\n",
    "n=int(input(\"enter a number:\"))\n",
    "original=n\n",
    "sum=0\n",
    "\n",
    "digits=len(str(n))\n",
    "\n",
    "while n>0:\n",
    "    digit=n%10\n",
    "    sum=sum + (digits ** digits)\n",
    "    n=n//10\n",
    "\n",
    "if sum==original:\n",
    "    print(\"it is armstrong number\")\n",
    "else:\n",
    "    print(\"it is not armstrong number\")\n",
    "    \n",
    "\n",
    "\n",
    "      "
