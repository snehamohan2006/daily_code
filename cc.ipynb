   "source": [
    "def count_digits(n):\n",
    "    count = 0\n",
    "\n",
    "    while n > 0:\n",
    "        count += 1\n",
    "        n = n // 10\n",
    "\n",
    "    return count\n",
    "\n",
    "\n",
    "# function call\n",
    "number = 1234\n",
    "result = count_digits(number)\n",
    "print(\"Number of digits:\", result)\n"
   ]
